#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/soocil/chada/grubstudio/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.0_typescript@5.8.3/node_modules/@commitlint/cli/node_modules:/home/soocil/chada/grubstudio/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.0_typescript@5.8.3/node_modules/@commitlint/node_modules:/home/soocil/chada/grubstudio/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.0_typescript@5.8.3/node_modules:/home/soocil/chada/grubstudio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/soocil/chada/grubstudio/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.0_typescript@5.8.3/node_modules/@commitlint/cli/node_modules:/home/soocil/chada/grubstudio/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.0_typescript@5.8.3/node_modules/@commitlint/node_modules:/home/soocil/chada/grubstudio/node_modules/.pnpm/@commitlint+cli@19.8.1_@types+node@24.0.0_typescript@5.8.3/node_modules:/home/soocil/chada/grubstudio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
